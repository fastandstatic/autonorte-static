				<div class="elementor-element elementor-element-df1ac8e elementor-widget elementor-widget-html" data-id="df1ac8e" data-element_type="widget" data-widget_type="html.default">
				<div class="elementor-widget-container">
			<link rel="stylesheet" href="//cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.min.css">
<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<script>
(function ($) {
    const formId = '#cotizar_auto_page';
    const evento = 'Formulario landing';

    const form = $(formId);
    if (!form.length) {
        console.error("No se encontr√≥ formulario con el ID");
    } else {
        // Agrega una funci√≥n para la validaci√≥n en tiempo real del campo de tel√©fono
        const telefonoInput = form.find('[name="form_fields[chat_phone]"]');
        telefonoInput.on('input', function () {
            // Remueve cualquier car√°cter que no sea un n√∫mero
            let valorActual = $(this).val().replace(/\D/g, '');
            // Asegura que el n√∫mero comience con 3
            if (!valorActual.startsWith('3')) {
                valorActual = '3' + valorActual;
            }
            // Limita el n√∫mero de d√≠gitos a 10
            if (valorActual.length > 10) {
                valorActual = valorActual.substring(0, 10);
            }
            $(this).val(valorActual);
        });

        form.on('submit', function (e) {
            e.preventDefault(); // Evita el env√≠o del formulario por defecto

            // Validar que el n√∫mero de tel√©fono tenga 10 d√≠gitos
            const telefonoValor = telefonoInput.val();
            if (telefonoValor.length !== 10) {
                Swal.fire({
                    title: "Error",
                    text: "El n√∫mero de tel√©fono debe tener exactamente 10 d√≠gitos.",
                    icon: "error",
                    confirmButtonColor: "#d33",
                    confirmButtonText: "Aceptar"
                });
                return; // Salir de la funci√≥n para no enviar el formulario
            }

            // Cerrar el popup de Elementor si est√° abierto
            const elementorPopup = $('.elementor-popup-modal');
            if (elementorPopup.length) {
                elementorPopup.fadeOut();
            }

            // Obtener la fecha y hora actual en un formato legible